{
    "headers": [
      {
        "source": "/Images/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/Images/(.*)",
        "destination": "/Images/$1"
      }
    ],
    "routes":[
        {
            "src": "/[^.]+",
            "dest": "/"
        }
    ]
  }
  